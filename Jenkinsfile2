pipeline {
agent any  
stages
{   stage ('Job is Prepairing for Print')
     {steps {sh 'echo "hello Jenkins Prepare to Print"'}}

    stage ('Job is Ready to Print')
     {steps {sh 'echo "hello jenkins is Ready For Print"'}}

    stage ('Job is Printing')
     {steps {sh 'echo "hello Jenkins is Printing The Job"'}}


}            
}
